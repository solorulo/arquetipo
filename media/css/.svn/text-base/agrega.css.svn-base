.tipo_imagen{ height: 100px; margin-right: auto; margin-left: auto; margin-top: 0px; margin-bottom: 0px; padding-top: 10px; padding-right: 0px; padding-bottom: 10px; padding-left: 0px; text-align: center; float: left; width: 140px; }
.tipo_imagen span, .creditos span{ clear: both; float: left; padding-left: 25px; margin-bottom: 3px; padding-bottom: 3px; }

.tipo_imagen input[type="radio"]{ float: left; margin-left: 2px; margin-right: 5px; }
.tipo_imagen label{ padding-top: 2px; float: left; }

