/*Drop area CSS */


.droparea { position: relative; text-align: center; float: left; width: 250px; height: 250px; font-size: 0.7em; clear: both; display: block; margin-right: 0%; margin-top: 5px; margin-bottom: 0px; margin-left: 10%; }
.droparea2, .droparea3 { text-align: center; font-size: 0.7em; display: block; height: 100px; width: 100px; position: relative; }
.multiple { position: relative; height: 20px; }
.droparea div, .droparea input, .multiple div, .multiple input, .droparea2 div, .droparea3 div { position: absolute; top: 0; width: 100%; height: 100%; }
.droparea input, .droparea2 input, .droparea3 input, .multiple input { cursor: pointer; opacity: 0; }
.droparea .instructions, .droparea2 .instructions, .droparea3 .instructions, .multiple .instructions { border: 2px dashed #ddd; opacity: .5; -webkit-transition: all .5s; -moz-transition: all .5s; transition: all .5s; }
.droparea .instructions.over, .droparea2 .instructions.over, .droparea3 .instructions.over, .multiple .instructions.over { border: 2px dashed #000; background: #229EC9; opacity: .9 }
.droparea .progress, .droparea2 .progress, .droparea3 .progress, .multiple .progress { position: absolute; bottom: 0; width: 100%; height: 0; color: #fff; background: #069; }
.multiple .progress { width: 0; height: 100%; }
